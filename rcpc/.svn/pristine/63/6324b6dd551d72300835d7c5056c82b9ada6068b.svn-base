  <select id="getAskinfoAndQuestionAndOption" parameterType="com.heag.rcpc.model.PcAskinfo" resultMap="AskinfoAndQuestionAndOptionsResultMap">
	SELECT a.aid,a.atitle,a.adescribe,a.aleadpage,a.atype,a.aendpage,a.alink,a.uname,a.isdelete,a.createdate,
	b.qid ,b.aid q_aid,b.qname q_qname,b.qtype q_qtype,b.orderid q_orderid,
	c.oid ,c.qid o_qid,c.ooption o_ooption,c.orderid o_orderid 
	FROM pc_askinfo a  LEFT JOIN  (SELECT * FROM pc_question WHERE isdelete =0) b ON a.aid = b.aid LEFT JOIN (SELECT* FROM pc_options 
	WHERE isdelete =0)c ON b.qid=c.qid WHERE 1=1 AND a.isdelete=0 and  a.aid = #{aid,jdbcType=INTEGER} ORDER BY qid, oid
